{
  "eslint.workingDirectories": [
    { "directory": "./client", "changeProcessCWD": true },
    { "directory": "./server", "changeProcessCWD": true }
  ],
  "files.exclude": {
    "client/node_modules": true,
    "client/coverage": true,
    "client/build": true,
    "client/**/.DS_Store": true,
    "client/**/.env.local": true,
    "client/**/.env.development.local": true,
    "client/**/.env.test.local": true,
    "client/**/.env.production.local": true,
    "client/**/npm-debug.log*": true,
    "client/**/yarn-debug.log*": true,
    "client/**/yarn-error.log*": true,
    "client/**/bundle-analysis-result.html": true,
    "**/.DS_Store": true,
    "**/.env": true,
    ".idea/*": true,
    "**/client/build/": true,
    "server/public": true,
    "**/coverage/": true,
    "**/db/": true,
    "**/dbtest/": true,
    "**/node_modules/": true,
    "**/sqlpad.tar.gz": true,
    "server/drivers/sqlite/sqlpad_test_sqlite.db": true,
    "server/test/artifacts": true,
    "server/test/fixtures/sales.sqlite": true,
    "client/.eslintcache": true
  }
}
