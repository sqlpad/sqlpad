{
  "name": "sqlpad",
  "version": "6.7.1",
  "description": "Web app. Write SQL and visualize the results. Supports Postgres, MySQL, SQL Server, Crate, Vertica, SAP HANA, and BigQuery.",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "sql",
    "mssql",
    "drill",
    "postgres",
    "postgresql",
    "mysql",
    "crate",
    "presto",
    "vertica",
    "sap",
    "hana",
    "bigquery",
    "bq"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/sqlpad/sqlpad"
  },
  "bugs": {
    "url": "https://github.com/sqlpad/sqlpad/issues"
  },
  "scripts": {
    "prepublishOnly": "cd .. && ./scripts/build.sh",
    "start": "node-dev server.js -- --config \"./config.dev.env\" | pino-pretty",
    "test": "mocha test --timeout 20000 --recursive --exit --bail",
    "testmssql": "env SQLPAD_BACKEND_DB_URI='mssql://sa:SuperP4ssw0rd!@localhost:1433/dbname' npm run test",
    "testmysql": "env SQLPAD_BACKEND_DB_URI='mysql://root:root@localhost:23306/db2' npm run test",
    "testmariadb": "env SQLPAD_BACKEND_DB_URI='mariadb://root:password@localhost:13306/db?timezone=Etc%2FGMT0&autoJsonMap=false' npm run test",
    "testpostgres": "env SQLPAD_BACKEND_DB_URI='postgres://sqlpad:sqlpad@localhost:5432/sqlpad' npm run test",
    "fixlint": "eslint --fix \"**/*.js\"",
    "lint": "eslint \"**/*.js\""
  },
  "dependencies": {
    "@google-cloud/bigquery": "^5.6.0",
    "@rickbergfalk/sequelize": "^6.6.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cassandra-driver": "^4.6.3",
    "clickhouse": "^2.4.0",
    "compression": "^1.7.4",
    "connect-redis": "^6.0.0",
    "connect-session-sequelize": "^7.1.0",
    "cryptr": "^6.0.2",
    "detect-port": "^1.3.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "express-session": "^1.17.2",
    "format-link-header": "^3.1.1",
    "hdb": "^0.18.2",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "ldapjs": "^2.3.0",
    "lodash": "^4.17.20",
    "lru-cache": "^6.0.0",
    "mariadb": "^2.5.2",
    "memorystore": "^1.6.6",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "mssql": "^6.3.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "node-crate": "^2.0.6",
    "node-fetch": "^2.6.1",
    "node-xlsx": "^0.16.1",
    "openid-client": "^4.7.4",
    "papaparse": "^5.3.1",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "passport-ldapauth": "^3.0.1",
    "passport-local": "^1.0.0",
    "passport-openidconnect": "0.0.2",
    "passport-saml": "^2.2.0",
    "pg": "^8.6.0",
    "pino": "^6.11.3",
    "prettier": "^2.3.1",
    "query-string": "^7.0.0",
    "redis": "^3.1.2",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "sanitize-filename": "^1.6.3",
    "sequelize": "npm:@rickbergfalk/sequelize@^6.6.5",
    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.5.0",
    "snowflake-sdk": "^1.6.1",
    "socksjs": "^0.5.0",
    "sql-formatter": "^2.3.3",
    "sql-limiter": "^2.6.0",
    "sqlite3": "^5.0.0",
    "umzug": "^2.3.0",
    "uuid": "^8.3.2",
    "vertica": "^0.5.5"
  },
  "main": "./app.js",
  "bin": {
    "sqlpad": "server.js"
  },
  "optionalDependencies": {
    "odbc": "^2.2.2"
  },
  "devDependencies": {
    "bytes": "3.1.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.0.0",
    "ncp": "^2.0.0",
    "node-dev": "^7.0.0",
    "parse-link-header": "^1.0.1",
    "pino-pretty": "^5.0.2",
    "supertest": "^6.1.1"
  }
}
