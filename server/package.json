{
  "name": "sqlpad",
  "version": "7.5.0",
  "description": "Web app. Write SQL and visualize the results. Supports Postgres, MySQL, SQL Server, Crate, Vertica, SAP HANA, and BigQuery.",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "keywords": [
    "sql",
    "mssql",
    "drill",
    "postgres",
    "postgresql",
    "mysql",
    "crate",
    "presto",
    "vertica",
    "sap",
    "hana",
    "bigquery",
    "bq"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/sqlpad/sqlpad"
  },
  "bugs": {
    "url": "https://github.com/sqlpad/sqlpad/issues"
  },
  "scripts": {
    "prepublishOnly": "cd .. && ./scripts/build.sh",
    "start": "node-dev server.js -- --config \"./config.dev.env\" | pino-pretty",
    "test": "mocha test --timeout 20000 --recursive --exit --bail",
    "testmssql": "env SQLPAD_BACKEND_DB_URI='mssql://sa:SuperP4ssw0rd!@localhost:1433/dbname' npm run test",
    "testmysql": "env SQLPAD_BACKEND_DB_URI='mysql://root:root@localhost:23306/db2' npm run test",
    "testmariadb": "env SQLPAD_BACKEND_DB_URI='mariadb://root:password@localhost:13306/db?autoJsonMap=false' npm run test",
    "testpostgres": "env SQLPAD_BACKEND_DB_URI='postgres://sqlpad:sqlpad@localhost:5432/sqlpad' npm run test",
    "fixlint": "eslint --fix \"**/*.js\" && prettier --write .",
    "lint": "eslint \"**/*.js\" && prettier --check ."
  },
  "dependencies": {
    "@clickhouse/client": "^0.0.16",
    "@google-cloud/bigquery": "^7.9.0",
    "@node-saml/passport-saml": "^5.0.0",
    "athena-express": "^7.1.5",
    "aws-sdk": "^2.1113.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cassandra-driver": "^4.6.3",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "connect-session-sequelize": "^7.1.3",
    "cryptr": "^6.0.2",
    "detect-port": "^1.3.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-pino-logger": "^7.0.0",
    "express-session": "^1.17.2",
    "format-link-header": "^3.1.1",
    "hdb": "^0.19.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "ldapjs": "^2.3.2",
    "lodash": "^4.17.20",
    "lru-cache": "^11.0.0",
    "mariadb": "^3.3.1",
    "memorystore": "^1.6.7",
    "minimist": "^1.2.6",
    "mkdirp": "^3.0.1",
    "moment": "^2.29.2",
    "mssql": "^11.0.1",
    "mustache": "^4.2.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.10.1",
    "node-crate": "^3.0.0",
    "node-fetch": "^3.3.2",
    "node-xlsx": "^0.24.0",
    "openid-client": "^5.3.1",
    "papaparse": "^5.3.2",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "passport-ldapauth": "^3.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pino": "^9.3.2",
    "prettier": "^2.6.2",
    "query-string": "^9.1.0",
    "redis": "^4.0.6",
    "rimraf": "^6.0.1",
    "sanitize-filename": "^1.6.3",
    "sequelize": "^6.19.0",
    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.5.0",
    "socksjs": "^0.5.0",
    "sql-formatter": "^2.3.3",
    "sql-limiter": "^3.0.0",
    "sqlite3": "^5.0.8",
    "umzug": "^2.3.0",
    "uuid": "^10.0.0",
    "vertica": "^0.5.5"
  },
  "main": "./app.js",
  "bin": {
    "sqlpad": "server.js"
  },
  "optionalDependencies": {
    "odbc": "^2.2.2"
  },
  "devDependencies": {
    "bytes": "^3.1.2",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^10.0.0",
    "ncp": "^2.0.0",
    "node-dev": "8.0.0",
    "parse-link-header": "^2.0.0",
    "pino-pretty": "^11.2.2",
    "supertest": "^7.0.0",
    "traverse": "^0.6.6"
  },
  "resolutions": {
    "supertest/**/cookiejar": "^2.1.4",
    "ip": "^2.0.1"
  }
}
