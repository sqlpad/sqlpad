    <script src="/javascripts/vendor/d3.v3.min.js"></script>
    <script src="/javascripts/vendor/dimple.v2.1.2.min.js"></script>
    <script src="/javascripts/vendor/jquery.splitter/jquery.splitter-0.14.0.js"></script>
